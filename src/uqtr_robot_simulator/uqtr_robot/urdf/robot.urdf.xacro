<?xml version="1.0" ?>

<robot name="Uqtr_Robot" xmlns:xacro="http://www.ros.org/wiki/xacro">

<!--Includes-->
<xacro:include filename="$(find uqtr_robot)/urdf/material.xacro" />
<xacro:include filename="$(find uqtr_robot)/urdf/robot.urdf" />
<xacro:include filename="$(find uqtr_robot)/urdf/properties.xacro" />

<!-- Friction pour les roues motrices-->
<xacro:Friction_roue_motrice wheel_name="roue_motrice_droite"/>
<xacro:Friction_roue_motrice wheel_name="roue_motrice_gauche"/>

<!-- Friction pour les roues caster-->
<xacro:Friction_roue_caster caster_name="chassis"/>


<!--Dummy-->

 <link name="base_link">
   </link>

   <joint name="dummy_joint" type="fixed">
     <origin
      xyz="0 0 0"
      rpy="0 0 0" />
     <parent link="base_link"/>
     <child link="chassis"/>
   </joint>


<!--Chassis-->
  <link
    name="chassis">
    <inertial>
      <mass
        value="49.497" />
     <inertia
        ixx="1.07"
        ixy="0"
        ixz="0.09"
        iyy="4.55"
        iyz="0"
        izz="5.30" />
    </inertial>
    <visual>
      <origin
        xyz="0 0 0"
        rpy="0 0 0" />
      <geometry>
        <mesh
          filename="package://uqtr_robot/meshes/chassis.dae" />
      </geometry>
    </visual>
    <collision>
      <origin
        xyz="0 0 0"
        rpy="0 0 0" />
      <geometry>
        <mesh
          filename="package://uqtr_robot/meshes/chassis.dae" />
      </geometry>
     <physics type="ode">
  <gravity xyz="0 0 -9.8"/>
  <ode>
    <solver type="quick" dt="0.001" iters="10" sor="1.3"/>
    <constraints cfm="0.0" erp="0.1" contact_max_correcting_vel="10.0" contact_surface_layer="0.001"/>
  </ode>
</physics>
    </collision>
  </link>


<!-- roue motrice droite -->

  <link
    name="roue_motrice_droite">
        <xacro:cylinder_inertia m="${masse_roue}" r="${rayon_roue}" h="${largeur_roue}"/>
    <visual>
      <origin
        xyz="0 0 0"
        rpy="0 0 0" />
      <geometry>
        <mesh
          filename="package://uqtr_robot/meshes/roue_motrice_droite.STL" />
      </geometry>
      <material
        name="">
        <color
          rgba="0.29804 0.29804 0.29804 1" />
      </material>
    </visual>
    <collision>
      <origin
        xyz="0 0 0"
        rpy="0 0 0" />
      <geometry>
        <mesh
          filename="package://uqtr_robot/meshes/roue_motrice_droite.STL" />
      </geometry>
    </collision>
  </link>

  <joint
    name="chassis_roue_motrice_droite"
    type="continuous">
    <origin
      xyz="0.074873 -0.34278 0.086706"
      rpy="0 0 0" />
    <parent
      link="chassis" />
    <child
      link="roue_motrice_droite" />
    <axis
      xyz="0 1 0" />
     <limit
      effort="${effort}"
      velocity="${velocity}" />
    <dynamics
      damping="${damping}"
      friction="${friction}" />
  </joint>
  
  <!-- roue motrice gauche -->

  <link
    name="roue_motrice_gauche">
        <xacro:cylinder_inertia m="${masse_roue}" r="${rayon_roue}" h="${largeur_roue}"/>
    <visual>
      <origin
        xyz="0 0 0"
        rpy="0 0 0" />
      <geometry>
        <mesh
          filename="package://uqtr_robot/meshes/roue_motrice_gauche.STL" />
      </geometry>
      <material
        name="">
        <color
          rgba="0.29804 0.29804 0.29804 1" />
      </material>
    </visual>
    <collision>
      <origin
        xyz="0 0 0"
        rpy="0 0 0" />
      <geometry>
        <mesh
          filename="package://uqtr_robot/meshes/roue_motrice_gauche.STL" />
      </geometry>
    </collision>
  </link>

  <joint
    name="chassis_roue_motrice_gauche"
    type="continuous">
    <origin
      xyz="0.074873 0.34278 0.086706"
      rpy="0 0 0" />
    <parent
      link="chassis" />
    <child
      link="roue_motrice_gauche" />
    <axis
      xyz="0 1 0" />
     <limit
      effort="${effort}"
      velocity="${velocity}" />
    <dynamics
      damping="${damping}"
      friction="${friction}" />
  </joint>

